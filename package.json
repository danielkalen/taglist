{
  "name": "taglist",
  "version": "2.2.0",
  "description": "Tag-style key:value lists with dynamic value calculation",
  "main": "dist/taglist.js",
  "browser": {
    "./debug": "dist/taglist.debug.js",
    "./dist/taglist.js": "src/index.coffee"
  },
  "browserify": {
    "transform": [
      "simplyimport/compat"
    ]
  },
  "simplyimport": {
    "finalTransform": [
      ".config/transforms/minify-super",
      ".config/transforms/minify-rename",
      ".config/transforms/minify-simple"
    ]
  },
  "scripts": {
    "postversion": "npm run build && git add . && git commit -a -m '[Build]'",
    "postpublish": "git push",
    "watch": "cake -d watch",
    "build": "cake -d build && cake build && cake measure && cp -r build/* dist/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielkalen/taglist.git"
  },
  "author": "danielkalen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/danielkalen/taglist/issues"
  },
  "homepage": "https://github.com/danielkalen/taglist#readme",
  "dependencies": {
    "@danielkalen/simplybind": "^1.15.8",
    "event-lite": "^0.1.1",
    "popper.js": "^1.14.3",
    "quickdom": "^1.0.76",
    "rxjs-compat": "^6.1.0"
  },
  "devDependencies": {
    "bluebird": "^3.5.0",
    "chalk": "^2.0.1",
    "coffee-script": "^1.12.6",
    "execa": "^0.7.0",
    "fs-jetpack": "^0.13.3",
    "promise-break": "^0.1.2",
    "semver": "^5.3.0",
    "smart-extend": "^1.7.3"
  }
}
